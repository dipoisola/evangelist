{
    "name": "league/evangelist",
    "description": "To check for developer status.",
    "keywords": [
        "league",
        "evangelist"
    ],
    "homepage": "https://github.com/thephpleague/evangelist",
    "license": "MIT",
    "authors": [
        {
            "name": "Dipo Isola",
            "email": "dipoisola@gmail.com",
            "homepage": "http://github.com/andela-oisola",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "knplabs/github-api": "^1.4",
        "php-mock/php-mock-phpunit": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "League\\Evangelist\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\EvangelistStatus\\Test\\": "tests"
        }
        "classmap": ["/src"]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
